{
    "name": "crm-backend",
    "version": "1.0.0",
    "description": "Backend project for crm-skautoteka-backend",
    "main": "index.js",
    "scripts": {
        "build": "npx tsc",
        "migrate": "npm run unseed:all && npm run seed:all",
        "dev:migrate": "npm run unseed:all && npm run seed:all && tsx watch index.ts",
        "dev": "tsx watch index.ts",
        "seed:all": "npx sequelize-cli db:seed:all --config seeders/config.json",
        "unseed:all": "npx sequelize-cli db:seed:undo:all --config seeders/config.json",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.3",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-fileupload": "^1.5.1",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.9.8",
        "node-fetch": "2.6.6",
        "nodemon": "^3.1.0",
        "pdfkit": "^0.16.0",
        "sequelize": "^6.37.3",
        "sequelize-typescript": "^2.1.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.5.0",
        "@types/bcrypt": "^5.0.2",
        "@types/body-parser": "^1.19.5",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-fileupload": "^1.5.1",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node-fetch": "^2.6.12",
        "@types/pdfkit": "^0.13.8",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^9.1.0",
        "globals": "^15.6.0",
        "prettier": "^3.3.2",
        "tsx": "^4.10.4",
        "typescript-eslint": "^7.13.1"
    }
}
